<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>fileTypes</key>
	<array>
		<string>svn-commit.tmp</string>
	</array>
	<key>name</key>
	<string>Subversion commit message</string>
	<key>patterns</key>
	<array>
		<dict>
			<key>begin</key>
			<string>(^--This line, and those below, will be ignored--$\n?)</string>
			<key>beginCaptures</key>
			<dict>
				<key>1</key>
				<dict>
					<key>name</key>
					<string>meta.diff.separator.svn</string>
				</dict>
			</dict>
			<key>end</key>
			<string>^--not gonna happen--$</string>
			<key>name</key>
			<string>meta.scope.changed-files.svn</string>
			<key>patterns</key>
			<array>
				<dict>
					<key>match</key>
					<string>^A\s+.*$\n?</string>
					<key>name</key>
					<string>markup.inserted.svn</string>
				</dict>
				<dict>
					<key>match</key>
					<string>^M\s+.*$\n?</string>
					<key>name</key>
					<string>markup.changed.svn</string>
				</dict>
				<dict>
					<key>match</key>
					<string>^D\s+.*$\n?</string>
					<key>name</key>
					<string>markup.deleted.svn</string>
				</dict>
			</array>
		</dict>
		<dict>
			<key>include</key>
			<string>text.plain</string>
		</dict>
	</array>
	<key>scopeName</key>
	<string>text.subversion-commit</string>
	<key>uuid</key>
	<string>5B201F55-90BC-4A69-9A44-1BABE5A9FE99</string>
</dict>
</plist>
